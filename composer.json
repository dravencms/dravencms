{
  "name": "dravencms/dravencms",
  "type": "library",
  "description": "DravenCMS Content Management System",
  "keywords": [
    "dravencms"
  ],
  "license": [
    "LGPL-3.0"
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@gitlab.salamek.cz:sadam/dravencms-template-college-green.git"
    },
    {
      "type": "vcs",
      "url": "git@gitlab.salamek.cz:sadam/dravencms-eshop.git"
    },
    {
      "type": "vcs",
      "url": "git@gitlab.salamek.cz:sadam/dravencms-template-bushido.git"
    }
  ],
  "authors": [
    {
      "name": "Adam Schubert"
    }
  ],
  "require-dev": {
    "nette/tester": "@dev"
  },
  "require": {
    "dravencms/packager": "dev-master",
    "dravencms/admin": "dev-master",
    "dravencms/issue": "dev-master"
  },
  "scripts": {
    "post-install-cmd": [
      "php vendor/dravencms/packager/bin/packager packager:sync"
    ],
    "post-update-cmd": [
      "php vendor/dravencms/packager/bin/packager packager:sync"
    ]
  },
  "minimum-stability": "dev"
}
