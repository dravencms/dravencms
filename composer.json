{
  "name": "dravencms/dravencms",
  "type": "library",
  "description": "DravenCMS Content Management System",
  "keywords": [
    "dravencms"
  ],
  "license": [
    "LGPL-3.0"
  ],
  "authors": [
    {
      "name": "Adam Schubert"
    }
  ],
  "require-dev": {
    "nette/tester": "@dev"
  },
  "require": {
    "php": ">= 7.3.0",
    "ext-openssl": "*",
    "ext-json": "*",
    "ext-gd": "*",
    "tracy/tracy": "^2.8",
    "dravencms/packager": ">=2.0"
  },
  "scripts": {
    "post-install-cmd": [
      "php vendor/dravencms/packager/bin/packager packager:sync"
    ],
    "post-update-cmd": [
      "php vendor/dravencms/packager/bin/packager packager:sync"
    ]
  }
}
